{
    "builds": [
      { "src": "./dist/server.js" , "use": "@vercel/node"},
      { "src": "build/*", "use": "@vercel/static" },
      { "src": ".next/*", "use": "@vercel/static" },
      { "src": "node_modules/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/dist" }
    ]
}